{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"types": ["node"],
		"incremental": true,
		"tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
		"jsx": "react-jsx",
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"paths": {
			"@/ui/*": ["./src/*"]
		},
		"allowImportingTsExtensions": true,
		"noEmit": true
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
